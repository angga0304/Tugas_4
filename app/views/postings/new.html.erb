<h1>Postings#new</h1>
<%= form_for @posting, html:{class:"form-horizontal", :multipart => true} do |f| %>
  <filedset>
    <%= f.file_field :photo %>
    <%= @posting.errors[:photo].first%>

    <div class="form-group">
      <%= f.label :title, class: "col-lg-1 control-label" %>
      <div class="col-lg-5">
        <%= f.text_field :title, cols: 20, rows: 10, class: "form-control" %>
      </div>
    </div>

    <div class="form-group">
      <%= f.label :userid, class: "col-lg-1 control-label" %>
      <div class="col-lg-5">
        <%= f.text_field :userid, class: "form-control" %>
      </div>
    </div>
    <%= f.submit :submit, class: "btn btn-primary" %>
  </fieldset>
<% end %>
